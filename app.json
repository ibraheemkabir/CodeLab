{
  "expo": {
    "name": "codelab-expo",
    "slug": "codelab-expo",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/iPhone.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "width" : 30,
      "height" : 30
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "ccom.kabir.codelab",
      "supportsTablet": true
    },
    "android": {
      "package": "com.kabir.codelab"
    },
    "scheme": "myapp",
    "description": "This app helps keep a list of all developers in lagos Nigeria"
  }
}